#include<iostream>
using namespace std;
int main(){
    int n,m;
    cout<<"enter size :";
    cin>>n>>m;
    int arr[n][m];
    cout<<"enter a elements:";
    for(int i=0;i<n;i++) {
        for(int j=0;j<m;j++){
            cin>>arr[i][j];
        }
    }
int x;
cout<<"enter search elements:";
cin>>x;
bool flag=false;
for(int i=0;i<n;i++){
    for(int j=0;j<m;j++){
        if(arr[i][j]==x){
            flag=true;
            
            cout<<"element is present"<<x;
            break;
        }
    }
}
        if(!flag){

            cout<<"element is not present";
         
        }
    
    }
    
     


    
