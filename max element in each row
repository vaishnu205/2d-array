#include<iostream>
#include<climits>
using namespace std;
int main(){
int n,m;
    cout<<"enter size of row:";
    cin>>m;
    cout<<"enter size of col:";
    cin>>n;
    int arr[m][n];
    for(int i=0;i<m;i++){
        for(int j=0;j<n;j++){
            cin>>arr[i][j];
        }
    }
    
    for(int i=0;i<m;i++){
        int maxval=INT_MIN;
        for(int j=0;j<n;j++){
            maxval=max(maxval,arr[i][j]);
        }
        cout<<i+1<<"="<<maxval<<endl;
    }
}
